{
  "expo": {
    "name": "QRコード作成",
    "description": "QRコードを作成するアプリです。",
    "slug": "qr-code-generator",
    "privacy": "public",
    "sdkVersion": "35.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#00a5ff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "qr.code.generator.nodokayamamoto.com"
    },
    "android": {
      "package": "qr.code.generator.nodokayamamoto.com"
    },
    "primaryColor": "#00a5ff"
  }
}
